@model dynamic

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <style>
        body {
            font-family: Arial;
            margin: 40px;
        }

        h1 {
            color: #007ACC;
        }

        .footer {
            margin-top: 50px;
            font-size: 0.9em;
            color: gray;
        }
    </style>
</head>
<body>
    <h1>PDF Report</h1>
    <p>Name: @Model.Name</p>
    <p>Date: @Model.Date.ToString("dd MMM yyyy")</p>

    <div class="footer">Generated with IronPDF in ASP.NET Core</div>
</body>
</html>
